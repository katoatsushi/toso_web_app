<div class="panel panel-primary">
  <% @mainparts.each do|main| %>
  <div class="panel-heading">
    <h3 class="panel-title"><%= main.mp_name  %></h3>
  </div>
    <% main.materials.each do |material| %>
      <div class="panel-body">
        <%= link_to material.mate_name ,new_building_mainpart_material_part_path(building_id: params[:building_id],mainpart_id: main.id,material_id: material.id) %>
      </div>
      <% end %>
  <% end %>
</div>